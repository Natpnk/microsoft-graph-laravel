{
    "name": "natpnk/microsoft-graph-laravel",
    "description": "Microsoft Graph wrapper and mail transport for Laravel.",
    "require": {
        "php": "^8.2",   
        "ext-json": "*",
        "illuminate/support": "^10.0|^11.0",
        "illuminate/mail": "^10.0|^11.0",
        "guzzlehttp/guzzle": "^7",
        "microsoft/microsoft-graph": "^1.30.0"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Natpnk\\MicrosoftGraphLaravel\\": "src"
        }
    },
    "extra": {
        "laravel": {            
            "providers": [
                "Natpnk\\MicrosoftGraphLaravel\\MicrosoftGraphServiceProvider"
            ],

            "aliases": {
                "MicrosoftGraph": "Natpnk\\MicrosoftGraphLaravel\\MicrosoftGraphFacade"
            }
        }
    },    
    "authors": [
        {
            "name": "Natpnk",
            "email": "patrick@natpnk.nl"
        }
    ]
}
